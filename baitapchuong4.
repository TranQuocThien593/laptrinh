def computgrade(score):
    # Nếu không phải số hoặc ngoài khoảng 0-1 → sai
    if score < 0 or score > 1:
        return "Bad score"
    
    if score > 0.9:
        return "A"
    elif score > 0.8:
        return "B"
    elif score > 0.7:
        return "C"
    elif score > 0.6:
        return "D"
    else:
        return "F"

# Phần chính của chương trình
try:
    s = float(input("Enter score: "))
    print(computgrade(s))
except:
    print("Bad score")
